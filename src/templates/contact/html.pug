mixin renderArray(arr)
  each val, key in arr
    .data-item
      p.array-item -
      +renderItem(val)

mixin renderItem(obj)
  each val, key in obj
    if val.constructor == Array
      .data-item
        p.name= key
        +renderArray(val)
    else if typeof(val) === 'object'
      .data-item
        p.name= key
        +renderItem(val)
    else
      .data-item
        p.name= key
        p.value= val

.wrapper
  h1#title= name
  h2#subtitle Nuevo contacto
  .data
    +renderItem(contact)
